#include<stdio.h>
int main()
{
  int a,b,c,max;
  printf("\nEnter 3 numbers");
  scanf("%d %d %d", &a, &b, &c);
  max=a;
  if(b>max)
   max=b;
  if(c>max) in
   max=c;
  printf("Largest number is %d", max);
  return 0;
}

